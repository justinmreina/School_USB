/************************************************************************************************************************************/
class Rectangle {
    int width, height;
  public:
    void set_values (int,int);
    int area () {return width*height;}
};


/************************************************************************************************************************************/
/**	@fcn		int main(void)
 *  @brief		x
 *  @details	x
 *
 *  @section	Purpose
 *  	x
 *
 *  @param		[in]	name	descrip
 *
 *  @param		[out]	name	descrip
 *
 *  @return		(type) descrip
 *
 *  @pre		x
 *
 *  @post		x
 *
 *  @section	Operation
 *		x
 *
 *  @section	Opens
 *  	x
 *
 *  @section	Hazards & Risks
 *  	x
 *
 *	@section	Todo
 *		x
 *
 *  @section	Timing
 *  	x
 *
 *  @note		x
 */
/************************************************************************************************************************************/
void Rectangle::set_values (int x, int y) {
  width = x;
  height = y;
}

